package comcast.test.app.testCases.misc.miscFunctions;

import org.openqa.selenium.By;

import comcast.test.app.common.XpathObjectRepo;
import comcast.test.config.configServices.utils.BaseTest;

public class MiscFun extends BaseTest {
	
	
	/**
	 * Method Name: clickOnBacktohomepageLink Description: This method used click
	 * On Back to home page Link from error page
	 * 
	 * @throwsdException
	 */
	public static void clickOnBacktohomepageLink() throws Exception {

		// click On Back to home page Link

		driver.findElement(By.xpath(XpathObjectRepo.pageNotFoundHomeLink_XPATH))
				.click();

		Thread.sleep(sleepTime);
		log.info("Successfully clicked On Back to home page Link");

	}

}
